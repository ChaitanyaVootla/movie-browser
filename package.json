{
  "name": "moviebrowser-nuxt",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview --host",
    "postinstall": "nuxt prepare",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:seo": "playwright test --project=\"SEO Tests (No JS)\"",
    "test:mobile": "playwright test --project=\"Mobile SEO\"",
    "test:content": "playwright test tests/e2e/ssr-content.spec.ts --project=\"SEO Tests (No JS)\"",
    "test:ssr-structure": "playwright test tests/e2e/ssr-content.spec.ts --grep=\"SSR Structure\" --project=\"SEO Tests (No JS)\"",
    "test:seo-full": "playwright test tests/e2e/seo.spec.ts tests/e2e/ssr-content.spec.ts --project=\"SEO Tests (No JS)\"",
    "test:all": "npm run test:unit && npm run test:seo-full",
    "test:ci": "npm run build && npm run test:unit && npm run test:e2e"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/image": "^1.11.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@playwright/test": "^1.55.0",
    "@sidebase/nuxt-auth": "^0.6.3",
    "@types/cheerio": "^0.22.35",
    "@types/country-list": "^2.1.4",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@vite-pwa/nuxt": "^0.10.6",
    "autoprefixer": "^10.4.16",
    "axios": "1.7.2",
    "cheerio": "^1.1.2",
    "happy-dom": "^18.0.1",
    "less": "^4.2.0",
    "nuxt": "^4.1.1",
    "nuxt-gtag": "^4.0.0",
    "nuxt-lodash": "^2.5.3",
    "nuxt-time": "^0.1.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.12",
    "vue-router": "^4.5.0",
    "vuetify": "^3.10.0"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.481.0",
    "@iconify/json": "^2.2.145",
    "@mdi/font": "^7.3.67",
    "@microsoft/clarity": "^1.0.0",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/core": "^11.0.3",
    "country-list": "^2.3.0",
    "lodash": "^4.17.21",
    "mongoose": "8.0.2",
    "next-auth": "4.21.1",
    "node-cron": "^3.0.3",
    "pinia": "^2.1.7",
    "xmlbuilder": "^15.1.1"
  }
}
